            "Wgnsy\\Currency\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wgnsy\\Currency\\CurrencyServiceProvider"
            ],
            "aliases": {
                "Currency": "Wgnsy\\Currency\\Facades\\Currency"
            }
        }
    }
}
