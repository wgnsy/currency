{
    "name": "wgnsy/currency",
    "description": "A Laravel Package Currency",
    "license": "MIT",
    "authors": [
        {
            "name": " wgnsy PRR WebCreate",
            "email": "wgnsy123@gmail.com"
        }
    ],
    "keywords": ["Laravel"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Wgnsy\\Currency\\": "src/"
        }
        
    },
